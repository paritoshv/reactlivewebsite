\n\r\n\r\n//Components\r\nimport Main from './components/Main';\r\nimport AboutPage from './components/AboutPage';\r\nimport BlogPage from './components/BlogPage';\r\nimport WorkPage from './components/WorkPage';\r\nimport MySkillsPage from './components/MySkillsPage';\r\nimport { AnimatePresence } from \"framer-motion\";\r\nimport SoundBar from \"./subComponents/SoundBar\";\r\n\r\n\r\nfunction App() {\r\n\r\n  const location = useLocation();\r\n  return <>\r\n\r\n\r\n  <GlobalStyle />\r\n\r\n    <ThemeProvider theme={lightTheme}>\r\n\r\n    <SoundBar />\r\n\r\n{/* For framer-motion animation on page change! */}\r\n<AnimatePresence exitBeforeEnter>\r\n<Switch  location={location} key={location.pathname}>\r\n      <Route exact path=\"/\" component={Main}/>\r\n      <Route exact path=\"/about\" component={AboutPage}/>\r\n      <Route exact path=\"/blog\" component={() => { \r\n     window.location.href = 'https://drive.google.com/file/d/1YbAqLgfEnj6z9R224GkYu3o1RfYkh1Me/view?usp=sharing' ; \r\n     return null;\r\n}} target=\"_blank\"/>\r\n      <Route exact path=\"/work\" component={WorkPage}/>\r\n      <Route exact path=\"/skills\" component={MySkillsPage}/>\r\n\r\n    </Switch>\r\n</AnimatePresence>\r\n    \r\n    \r\n    </ThemeProvider>\r\n\r\n\r\n    \r\n    </>\r\n    \r\n}\r\n\r\nexport default App\r\n\r\n","D:\\portfolio\\react-portfolio-final\\src\\globalStyles.js",[],"D:\\portfolio\\react-portfolio-final\\src\\components\\BlogPage.js",[],"D:\\portfolio\\react-portfolio-final\\src\\components\\WorkPage.js",[],"D:\\portfolio\\react-portfolio-final\\src\\components\\Themes.js",[],"D:\\portfolio\\react-portfolio-final\\src\\components\\AboutPage.js",[],"D:\\portfolio\\react-portfolio-final\\src\\components\\Main.js",[],"D:\\portfolio\\react-portfolio-final\\src\\components\\MySkillsPage.js",["93"],"import React from 'react'\r\nimport styled, { ThemeProvider } from 'styled-components'\r\nimport {lightTheme} from './Themes';\r\nimport { Design, Develope} from './AllSvgs';\r\n\r\n\r\nimport LogoComponent from '../subComponents/LogoComponent';\r\nimport SocialIcons from '../subComponents/SocialIcons';\r\nimport PowerButton from '../subComponents/PowerButton';\r\nimport ParticleComponent from '../subComponents/ParticleComponent';\r\nimport BigTitle from '../subComponents/BigTitlte'\r\n\r\nconst Box = styled.div`\r\nbackground-color: ${props => props.theme.body};\r\nwidth: 100vw;\r\nheight:110vh;\r\nposition: relative;\r\ndisplay: flex;\r\njustify-content: space-evenly;\r\nalign-items: center;\r\n\r\n\r\n`\r\n\r\nconst Main = styled.div`\r\nborder: 2px solid ${props => props.theme.text};\r\ncolor: ${props => props.theme.text};\r\nbackground-color: ${props => props.theme.body};\r\npadding: 2rem;\r\nwidth: 30vw;\r\nheight: 65vh;\r\nz-index:3;\r\nline-height: 1;\r\ncursor: pointer;\r\n\r\nfont-family: 'Ubuntu Mono',monospace;\r\ndisplay: flex;\r\nflex-direction: column;\r\njustify-content: space-between;\r\n\r\n&:hover{\r\n    color: ${props => props.theme.body};\r\n    background-color: ${props => props.theme.text};\r\n}\r\n`\r\n\r\nconst Title = styled.h2`\r\ndisplay: flex;\r\njustify-content: center;\r\nalign-items: center;\r\nfont-size: calc(1em + 1vw);\r\n\r\n${Main}:hover &{\r\n    &>*{\r\n        fill:${props => props.theme.body};\r\n    }\r\n}\r\n\r\n&>*:first-child{\r\nmargin-right: 1rem;\r\n}\r\n`\r\n\r\nconst Description = styled.div`\r\ncolor: ${props => props.theme.text};\r\nfont-size: calc(0.6em + 1vw);\r\npadding: 0.3rem 0;\r\n\r\n\r\n${Main}:hover &{\r\n   \r\n        color:${props => props.theme.body};\r\n    \r\n}\r\n\r\nstrong{\r\n    margin-bottom: 1rem;\r\n    text-transform: uppercase;\r\n}\r\nul,p{\r\n    margin-left: 2rem;\r\n}\r\n`\r\n\r\nconst MySkillsPage = () => {\r\n    return (\r\n        <ThemeProvider theme={lightTheme}>\r\n<Box>\r\n\r\n<LogoComponent theme='light'/>\r\n<SocialIcons theme='light'/>\r\n<PowerButton />\r\n<ParticleComponent theme='light' />\r\n            <Main>\r\n<Title>\r\n    <Design width={40} height={40} /> Designer\r\n</Title>\r\n<Description>\r\nI love to create design which are minimal and simple yet Engaging and attention pulling!\r\n</Description>\r\n<Description>\r\n<strong>Softwares I Use:</strong>\r\n<ul>\r\n    <li>\r\n        Adobe Photoshop, Illustrator, Figma\r\n    </li>\r\n    <li>\r\n        Adobe After Effects,Premier Pro\r\n    </li>\r\n    <li>\r\n        Blender\r\n    </li>\r\n</ul>\r\n</Description>\r\n<Description>\r\n<strong>Design PortFolio:</strong>\r\n<ul>\r\n    <a href=\"https://drive.google.com/drive/folders/1ZiddZC0UW9Z9fHjfm9D_H0vfhfhOqb-e?usp=sharing\" target=\"_blank\" >\r\n       Link\r\n    </a>\r\n    \r\n</ul>\r\n</Description>\r\n\r\n            </Main>\r\n            <Main>\r\n<Title>\r\n    <Develope width={40} height={40} /> Developer/Engineer\r\n</Title>\r\n<Description>\r\nI have Basic Programming Knowledge & I keep learning with every Project I do.\r\n</Description>\r\n<Description>\r\n<strong>Skills:</strong>\r\nC, Python, Html, Css, Js, React, Bootstrap and learning\r\n</Description>\r\n<Description>\r\n<strong>Internships:</strong>\r\nPyrotech Electronics(Practice School):Designed and Developed data Visualization components for SCADA systems, using React js, Syncfusion, CSS, and JavaScript.\r\nComponents included: Real-time line, Bar Graphs, Pie charts and Temprature displaying Flipcards.\r\n\r\n</Description>\r\n\r\n            </Main>\r\n\r\n            <BigTitle text=\"SKILLS & Experience\" top=\"80%\" right=\"30%\" />\r\n\r\n        </Box>\r\n\r\n        </ThemeProvider>\r\n        \r\n    )\r\n}\r\n\r\nexport default MySkillsPage\r\n","D:\\portfolio\\react-portfolio-final\\src\\subComponents\\SoundBar.js",[],"D:\\portfolio\\react-portfolio-final\\src\\subComponents\\SocialIcons.js",[],"D:\\portfolio\\react-portfolio-final\\src\\subComponents\\LogoComponent.js",[],"D:\\portfolio\\react-portfolio-final\\src\\subComponents\\PowerButton.js",[],"D:\\portfolio\\react-portfolio-final\\src\\subCo